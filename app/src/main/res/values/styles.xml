<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="TextStyleToolbar" parent="TextAppearance.AppCompat.Light.SearchResult.Title">
        <item name="android:textSize">@dimen/medium_font_size</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight" tools:targetApi="p">500</item>
    </style>


    <style name="ButtonError" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">bottom|center_horizontal</item>
        <item name="android:backgroundTint">?attr/colorOnPrimary</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="cornerRadius">54dp</item>
        <item name="android:textColor">?attr/colorSecondaryVariant</item>
        <item name="android:textFontWeight" tools:targetApi="p">500</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:elevation">0dp</item>
    </style>


    <style name="TextSettings" parent="Widget.AppCompat.TextView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/margin_16</item>
        <item name="android:layout_marginEnd">@dimen/margin_8</item>
        <item name="android:layout_marginTop">@dimen/margin_21</item>
        <item name="android:layout_marginBottom">@dimen/margin_21</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textFontWeight" tools:targetApi="p">400</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">@dimen/regular_font_size</item>
    </style>

    <style name="ImagesViewSettings">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">@dimen/margin_12</item>
        <item name="android:layout_marginTop">@dimen/margin_19</item>
        <item name="android:layout_marginBottom">@dimen/margin_18</item>
        <item name="android:layout_gravity">center_vertical|end</item>
        <item name="tint">?attr/colorOnSecondary</item>

    </style>

    <style name="TextMedium">parent="Widget.AppCompat.TextView">
        <item name="android:textFontWeight" tools:targetApi="p">400</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">@dimen/regular_font_size</item>
    </style>

    <style name="TextSmall">parent="Widget.AppCompat.TextView">
        <item name="android:textFontWeight" tools:targetApi="p">400</item>
        <item name="android:textColor">?attr/colorOnSecondary</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">11sp</item>
    </style>

    <style name="TextStyleError">
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">?attr/colorSecondary</item>
        <item name="android:textSize">@dimen/error_font_size</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight" tools:targetApi="p">500</item>
    </style>

    <style name="TextStylePlayer" parent="TextAppearance.AppCompat.Light.SearchResult.Title">
        <item name="android:textSize">@dimen/medium_font_size</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight" tools:targetApi="p">400</item>
    </style>

    <style name="TextStylePlayerSmall" parent="TextAppearance.AppCompat.Light.SearchResult.Title">
        <item name="android:textSize">13sp</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight" tools:targetApi="p">400</item>
    </style>

    <style name="TextStylePlayerSmallLeft" parent="TextAppearance.AppCompat.Light.SearchResult.Title">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight" tools:targetApi="p">400</item>
    </style>

    <style name="TextStyleMedium14_500" parent="TextAppearance.AppCompat.Light.SearchResult.Title">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight" tools:targetApi="p">500</item>
    </style>

    <style name="TextStyleMedium19_400" parent="TextAppearance.AppCompat.Light.SearchResult.Title">
        <item name="android:textSize">19sp</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight" tools:targetApi="p">400</item>
    </style>

    <style name="StyleMenuNavigation">
        <item name="android:background">@color/white</item>
    </style>

    <style name="TextStyleRegular12_400" parent="TextAppearance.AppCompat.Light.SearchResult.Title">
    <item name="android:textSize">12sp</item>
    <item name="android:fontFamily">@font/ys_display_regular</item>
    <item name="android:textFontWeight" tools:targetApi="p">400</item>
</style>

</resources>
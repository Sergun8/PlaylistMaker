<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="MessageTextStyle">
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:textSize">@dimen/medium_font_size</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>

    <style name="ButtonMainActivity" parent="Widget.MaterialComponents.Button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_marginStart">@dimen/margin_17</item>
        <item name="android:layout_marginEnd">@dimen/margin_15</item>
        <item name="android:layout_marginBottom">@dimen/margin_16</item>
        <item name="android:layout_weight">1</item>
        <item name="android:backgroundTint">?attr/colorSecondary</item>
        <item name="android:textColor">?attr/colorPrimary</item>
        <item name="android:textSize">@dimen/medium_font_size</item>
        <item name="android:textAllCaps">false</item>
        <item name="iconGravity">textStart</item>
        <item name="cornerRadius">@dimen/cornerRadius_10</item>
        <item name="android:textFontWeight" tools:targetApi="p">500</item>
        <item name="iconTint">?attr/colorPrimary</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:focusable">false</item>
    </style>




</resources>